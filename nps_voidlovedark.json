{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "name": "Violet Shadow Purity – roxo intenso, presença pura e harmoniosa",
  "colors": {
    "foreground": "#EAD8FF",
    "editor.background": "#07001F",
    "editor.foreground": "#F0E6FF",

    "editorCursor.foreground": "#FF66FF",
    "editor.lineHighlightBackground": "#1A0033",
    "editor.selectionBackground": "#33006655",
    "editorWhitespace.foreground": "#A066FF33",

    "editorLineNumber.foreground": "#6E4D9A",
    "editorLineNumber.activeForeground": "#D7B5FF",
    "editorBracketMatch.border": "#D88EFF",

    "activityBar.background": "#0A001F",
    "activityBar.foreground": "#C6A0FF",
    "activityBarBadge.background": "#D88EFF",
    "activityBarBadge.foreground": "#000000",

    "sideBar.background": "#0D0024",
    "sideBar.foreground": "#F0E6FF",

    "tab.activeBackground": "#120033",
    "tab.inactiveBackground": "#0D0024",
    "tab.activeForeground": "#E8CFFF",
    "tab.inactiveForeground": "#8F7CAA",

    "statusBar.background": "#0B001F",
    "statusBar.foreground": "#C6A0FF",
    "statusBar.debuggingBackground": "#D88EFF",
    "statusBar.debuggingForeground": "#000000",

    "titleBar.activeBackground": "#0A001F",
    "titleBar.activeForeground": "#E8CFFF",

    "terminal.background": "#07001F",
    "terminal.foreground": "#F0E6FF",
    "terminalCursor.foreground": "#D88EFF",

    "terminal.ansiRed": "#FF66AA",
    "terminal.ansiGreen": "#9AFFD0",
    "terminal.ansiYellow": "#FFE0FF",
    "terminal.ansiBlue": "#A77BFF",
    "terminal.ansiMagenta": "#D799FF",
    "terminal.ansiCyan": "#78F6FF",
    "terminal.ansiWhite": "#FFFFFF",

    "errorForeground": "#FF4DAA"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#9B5FA8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#D8A3FF"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#E085FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#D899FF"
      }
    },
    {
      "scope": "number",
      "settings": {
        "foreground": "#B48EFF"
      }
    },
    {
      "scope": "function",
      "settings": {
        "foreground": "#AC7EFF"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#E0A0FF"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#E085FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "type",
      "settings": {
        "foreground": "#D899FF"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#A48CBF"
      }
    },
    {
      "scope": "meta",
      "settings": {
        "foreground": "#CFC3E6"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#7A008A"
      }
    }
  ]
}

